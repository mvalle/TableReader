open_file = open("test.csv")

first_line = open_file.readlines()

headers = first_line.split(',')

attributes = {}
attribute_map = {}

for (i, header) in enumerte(headers):
    attributes[header] = ""
    attribute_map[i] = header
attributes["__attribute_map__"] = attribute_map

Csv = type("Csv", (), attributes)

print Csv 
